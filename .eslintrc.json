{
  "extends": "notninja/es5",
  "env": {
    "node": true
  },
  "rules": {
    "no-empty-function": "off",
    "no-invalid-this": "off",
    "max-params": "off",
    "no-undefined": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "valid-jsdoc": [
      "error",
      {
        "requireParamDescription": false,
        "requireReturnDescription": false
      }
    ]
  }
}
